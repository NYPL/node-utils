{
  "name": "@nypl/node-utils",
  "version": "1.0.1",
  "description": "A collection of common utilities for NYPL Node apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NYPL/node-utils.git"
  },
  "scripts": {
    "build": "rollup -c && npm run build:types",
    "build:types": "tsc -p tsconfig.declarations.json",
    "test": "node --import tsx --test test/**.test.ts"
  },
  "keywords": [],
  "author": "@nonword",
  "files": [
    "./dist/*"
  ],
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-kms": "^3.926.0",
    "winston": "^3.18.3",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.14.1",
    "rollup": "^4.40.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  }
}
